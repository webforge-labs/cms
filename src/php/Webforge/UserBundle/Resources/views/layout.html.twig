{%include 'WebforgeCmsBundle:default:head.html.twig' %}
<body>

  <div class="container">
    <div class="row">
      <div class="col-xs-10 col-sm-8 col-md-8 col-lg-6">
        {% for type, messages in app.session.flashbag.all() %}
            {% for message in messages %}
                <div class="alert alert-{{type}} flash-{{ type }}" role="alert">
                    {{ message }}
                </div>
            {% endfor %}
        {% endfor %}

        {% block fos_user_content %}
        {% endblock fos_user_content %}
      </div>
    </div>
  </div>

  <script type="text/javascript">
      require(['cms/login']);
  </script>

{%include 'WebforgeCmsBundle:default:foot.html.twig' %}